{
  "name": "@galleondao/setswap-tokenlist",
  "version": "0.0.1",
  "description": "token lists used by SetSwap",
  "main": "./build/cjs/index.js",
  "types": "./build/cjs/index.d.ts",
  "module": "./build/esm/index.js",
  "files": [
    "build/"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test": "jest",
    "lint": "gts lint",
    "clean": "rimraf build",
    "commit": "git-cz",
    "compile:esm": "tsc -p tsconfig.esm.json",
    "compile:cjs": "tsc -p tsconfig.cjs.json",
    "fix": "gts fix",
    "prepare": "husky install && npm run build",
    "posttest": "npm run lint",
    "build": "npm run compile:esm && npm run compile:cjs",
    "semantic-release": "semantic-release --branches main",
    "acp": "git add . && npm run commit && git push"
  },
  "keywords": [
    "npm",
    "example",
    "react",
    "hooks",
    "commonjs",
    "es6"
  ],
  "homepage": "https://github.com/GalleonDAO/setswap-tokenlist",
  "repository": {
    "type": "git",
    "url": "https://github.com/GalleonDAO/setswap-tokenlist"
  },
  "bugs": {
    "url": "https://github.com/GalleonDAO/setswap-tokenlist/issues"
  },
  "author": {
    "name": "Andrew",
    "email": "hello@galleon.community",
    "url": "https://galleon.community"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8",
    "react-dom": "^16.8"
  },
  "devDependencies": {
    "@testing-library/react": "^11",
    "@testing-library/react-hooks": "^7",
    "@types/jest": "^26",
    "@types/node": "^14",
    "@types/react": "^16.9",
    "@types/react-dom": "^16.9",
    "cz-conventional-changelog": "^3.3.0",
    "gts": "^3",
    "husky": "^6.0.0",
    "jest": "^27",
    "prettier": "^2",
    "react": "^16.8",
    "react-dom": "^16.8",
    "react-test-renderer": "^16.14.0",
    "rimraf": "^3",
    "semantic-release": "^18.0.1",
    "ts-jest": "^27",
    "typescript": "^4.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
